<h1>Lists#show</h1>
<p>Find me in app/views/lists/show.html.erb</p>

<div class="record-detail">
	<div class="name-value">
		<label>ID</label>
		<p><%=@list.id %></p>
	</div>
	<div class="name-value">
		<label>Title</label>
		<p><%=@list.title %></p>
	</div>
	<div class="name-value">
		<label>Description</label>
		<p><%=@list.description %></p>
	</div>
	<div class="name-value">
		<label>Created At</label>
		<p><%=@list.created_at %></p>
	</div>
	<div class="name-value">
		<label>Update At</label>
		<p><%=@list.updated_at %></p>
	</div>
</div>

<div>
  <% @list.listings.each do |listing| %>
    <p><%= "#{listing.ordering} #{listing.repository.full_name}" %><br/>
    <%= "#{listing.commentary}" %></p>
  <% end %>
</div>

<br/>
<%= link_to "lists", lists_path %>
<br/>
<%= link_to "edit", edit_list_path %>
