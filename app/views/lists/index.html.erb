<h1>Lists#index</h1>
<p>Find me in app/views/lists/index.html.erb</p>

<table>
	<thead>
		<tr>
			<th>Id</th>
			<th>Title</th>
			<th>Created At</th>
			<th>Updated At</th>
			<th>Repository Count</th>
			<th>Comments</th>
		</tr>
	</thead>
	<tbody>
	<% @lists.each do |list| %>
		<tr>
			<td><%= link_to list.id, list %></td>
			<td><%= list.title %></td>
			<td><%= list.created_at %></td>
			<td><%= list.updated_at %></td>
			<td>todo</td>
			<td>todo</td>			
		</tr>  
	<% end %>
	</tbody>
</table>
