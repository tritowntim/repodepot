<h1>Lists#new</h1>
<p>Find me in app/views/lists/new.html.erb</p>

<%= form_for(@list) do |f| %>

	<div class="field">
		<%= f.label :title %><br />
		<%= f.text_field :title %>
	</div>
	<div class="field">
		<%= f.label :description %><br />
		<%= f.text_field :description %>
	</div>
	<div class="actions">
		<%= f.submit %>
	</div>

<% end %>